<launch>

 <!-- joy node -->
  <node respawn="true" pkg="joy"
        type="joy_node" name="turtle_joy" >
    <param name="dev" type="string" value="/dev/input/js2" />
    <param name="deadzone" value="0.12" />
  </node>

  <!-- teleop control -->
  <node pkg="ros_teleop" type="teleop_control.py" name="teleop_control"/>

</launch>
